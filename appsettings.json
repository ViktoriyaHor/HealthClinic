{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "HealthClinicDbContext": "Server=localhost,1433;Database=HealthClinicDB;User Id={DB_USER};Password={DB_PASSWORD};TrustServerCertificate=True;"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "./certificate.pfx",
          "Password": "<CERTIFICATE_PASSWORD>"
        }
      }
    }
  }
}
